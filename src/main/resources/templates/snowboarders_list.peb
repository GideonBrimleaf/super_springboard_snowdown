{% extends 'templates/app.peb' %}

{% block content %}
  <main>
    <h1>My List of Snowboarders:</h1>
    <ul>
      {% for snowboarder in snowboarders  %}
        <li>
            <h2>{{ snowboarder.name }}</h2>
            <h3>{{ snowboarder.age }}</h3>
            <img src="{{ snowboarder.profile }}" alt="{{ snowboarder.name }} profile"></img>
        </li>
      {% endfor %}
    </ul>
  </main>
{% endblock %}